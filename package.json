{
  "name": "word-finder",
  "version": "1.0.3",
  "description": "find words in English dictionary with autosuggestion!",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node test/test.js",
    "nocov": "node test/*.js",
    "coverage": "./node_modules/.bin/istanbul cover test/*.js && ./node_modules/.bin/istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=1c0b40358b6943d3ecd94d6254d5cd2ac5dfc51188b4019bfca83b9b8a3542e2 ./node_modules/codeclimate-test-reporter/bin/codeclimate.js < ./coverage/lcov.info",
    "jshint": "jshint -c .jshintrc --exclude-path .gitignore .",
    "start": "node server.js",
    "nodemon": "./node_modules/.bin/nodemon ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dwyl/autocomplete.git"
  },
  "keywords": [
    "autocomplete",
    "english",
    "dictionary",
    "suggestions"
  ],
  "author": "@nelsonic github.com/nelsonic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dwyl/autocomplete/issues"
  },
  "homepage": "https://github.com/dwyl/autocomplete",
  "devDependencies": {
    "@tapjs/typescript": "^2.0.3",
    "nodemon": "^3.1.4",
    "nyc": "^17.0.0",
    "tap": "^20.0.3",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3"
  },
  "pre-commit": [
    "jshint",
    "coverage",
    "codeclimate"
  ],
  "engines": {
    "node": ">= 20"
  }
}
